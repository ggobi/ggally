> ## Simple version, no error checking, no derivative, nothing:
> psiGGW <- function(x, a,b,c) {
+     ifelse((ax <- abs(x)) < c,
+            x,
+            ifelse((ea <- -((ax-c)^b)/(2*a)) < -708.4, 0, x * exp(ea)))
+ }
> assert.EQ(Mpsi  (5:9, cc=c(0, a=1/8,b=2,c=1/8, NA), "GGW"),
